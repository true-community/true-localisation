<?xml version="1.0" encoding="windows-1251"?>
<string_table>
  <string id="st_mkat_mercs_rupor_start_1">
    <text>Listen up, stalkers!</text>
  </string>
  <string id="st_mkat_mercs_rupor_start_2">
    <text>If you want to live, find weapons and get to killing each other.</text>
  </string>
  <string id="st_mkat_mercs_rupor_start_3">
    <text>Only one leaves these tunnels alive!</text>
  </string>
  <string id="st_mkat_mercs_rupor_start_4">
    <text>Whoever tries anything gets a bullet in the head.</text>
  </string>
  <string id="st_mkat_mercs_rupor_start_5">
    <text>We're watching you!</text>
  </string>
  <string id="st_mkat_mercs_rupor_start_6">
    <text>Don't let us down, the stakes are high on you!</text>
  </string>
  <string id="st_mkat_merc_leader_see_actor_start">
    <text>Now turn around slowly, jarhead.</text>
  </string>
  <string id="st_mkat_merc_leader_after_dialog_info_1">
    <text>Don't scheme behind my back.</text>
  </string>
  <string id="st_mkat_merc_leader_after_dialog_info_2">
    <text>Nothing will come out of it anyway.</text>
  </string>
  <string id="st_mkat_merc_leader_actor_hit_angry">
    <text>Soon you'll pay for that!</text>
  </string>
</string_table>
